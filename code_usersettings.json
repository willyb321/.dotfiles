{
	"workbench.iconTheme": "vscode-icons",
    "editor.fontFamily": "'Fira Code'",
    "editor.fontSize": 14,
    "editor.fontLigatures": true,
	"material-icon-theme.showUpdateMessage": false,
	"vsicons.dontShowNewVersionMessage": true,
	"extensions.autoUpdate": false,
	"files.eol": "\n",
	"editor.insertSpaces": false,
	"html.format.endWithNewline": true,
	"files.insertFinalNewline": true,
	"editor.folding": false,
	"editor.dragAndDrop": false,
	"files.trimTrailingWhitespace": true,
	"explorer.openEditors.visible": 0,
	"explorer.openEditors.dynamicHeight": false,
	"html.format.indentInnerHtml": true,
	"editor.colorDecorators": true,
	"telemetry.enableTelemetry": false,
	"markdown-toc.updateOnSave": false,
	"npm-intellisense.importLinebreak": ";\n",
	"npm-intellisense.importES6": true,
	"material-icon-theme.showWelcomeMessage": false,
	"workbench.colorTheme": "Material Theme",
	"materialTheme.cache.workbench.settings": {
		"themeColours": "Default"
	},
	"editor.find.autoFindInSelection": true,
	"editor.quickSuggestions": {
		"other": true,
		"comments": false,
		"strings": false
	},
	"editor.wordBasedSuggestions": false,
	"editor.renderWhitespace": "boundary",
	"editor.renderIndentGuides": true,
	"editor.trimAutoWhitespace": true,
	"window.openFoldersInNewWindow": "off",
	"files.trimFinalNewlines": true,
	"zenMode.fullScreen": false,
	"zenMode.hideTabs": false,
	"zenMode.hideStatusBar": false,
	"zenMode.hideActivityBar": false,
	"zenMode.restore": false,
	"html.format.preserveNewLines": true,
	"html.format.indentHandlebars": true,
	"html.autoClosingTags": false,
	"json.format.enable": true,
	"typescript.disableAutomaticTypeAcquisition": false,
	"typescript.validate.enable": true,
	"typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBraces": false,
	"jsDocCompletion.enabled": true,
	"typescript.tsc.autoDetect": "on",
	"extensions.ignoreRecommendations": true,
	"terminal.explorerKind": "integrated",
	"telemetry.enableCrashReporter": false,
	"material-icon-theme.angular.iconsEnabled": true,
	"jake.autoDetect": "on",
	"grunt.autoDetect": "on",
	"npm.runInTerminal": true,
	"markdown-toc.insertAnchor": true,
	"markdown-toc.anchorMode": "github.com",
	"markdown-toc.withLinks": true,
	"auto-close-tag.activationOnLanguage": [
		"xml",
		"php",
		"blade",
		"ejs",
		"jinja",
		"markdown",
		"vue",
		"liquid",
		"erb"
	],
	"auto-close-tag.SublimeText3Mode": true,
	"docthis.includeDescriptionTag": true,
	"docthis.includeAuthorTag": true,
	"docthis.authorName": "Willyb321",
	"docthis.inferTypesFromNames": true,
	"pugbeautify.fillTab": true,
	"npm-intellisense.packageSubfoldersIntellisense": true,
	"npm-intellisense.importDeclarationType": "const",
	"gulp.autoDetect": "on",
	"git.enableCommitSigning": true
}

